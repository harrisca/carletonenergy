<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zephyrus.testapp.carletonenergyapp.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.zephyrus.testapp.carletonenergyapp.app.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zephyrus.testapp.carletonenergyapp.app.WindActivity"
            android:label="@string/title_activity_wind"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:name="com.zephyrus.testapp.carletonenergyapp.app.DataActivity"
            android:label="@string/title_activity_data" >
        </activity>
        <activity
            android:name="com.zephyrus.testapp.carletonenergyapp.app.EnergyActivity"
            android:label="@string/title_activity_energy"
            android:screenOrientation="sensorPortrait">
        </activity>
        <activity
            android:name="com.zephyrus.testapp.carletonenergyapp.app.GraphActivity"
            android:label="@string/title_activity_graph" >
        </activity>
        <activity
            android:name="com.zephyrus.testapp.carletonenergyapp.app.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:screenOrientation="sensorPortrait">
        </activity>
    </application>

</manifest>
